{
    "name": "unknownrori/easyarray",
    "description": "A wrapper class that manipulate array easy to read while still lightweight and fast",
    "authors": [
        {
            "name": "UnknownRori"
        }
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "UnknownRori\\EasyArray\\": "src/"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "UnknownRori\\Test\\EasyArray\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpbench/phpbench": "^1.2"
    },
    "scripts": {
        "test": "phpunit",
        "bench": "phpbench run tests --report=default"
    }
}
